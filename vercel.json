{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "@api_base_url",
    "NEXT_PUBLIC_FRONTEND_URL": "@frontend_url"
  },
  "project": {
    "framework": "nextjs",
    "rootDirectory": "frontend"
  },
  "functions": {
    "frontend/app/**": {
      "maxDuration": 30
    }
  }
}